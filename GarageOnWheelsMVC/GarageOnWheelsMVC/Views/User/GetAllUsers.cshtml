@model IEnumerable<GarageOnWheelsMVC.Models.User>

@{
    ViewData["Title"] = "Users";

}

<div class="container mt-5">
    <h1>@ViewData["Title"]</h1>

    <div class="row mb-3">
        <div class="col">
            <a class="btn btn-secondary" href="@Url.Action("Create", "User")"> + User</a>
        </div>
    </div>
    <div class="table-responsive">
        @Html.Partial("_UserListPartial", Model)

    </div>

 
</div>

    @section Scripts {

        <script>
            $(document).ready(function () {
                var message = '@TempData["Successful"]';
                if (message) {
                    toastr.success(message);
                }
          
            });
        </script>
    }